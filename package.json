{
  "name": "dazv",
  "version": "1.0.0",
  "description": "canvas data visualization",
  "main": "build/calendar.js",
  "scripts": {
    "mini": "npm run build && uglifyjs build/calendar.js -c -m -o build/calendar.min.js",
    "build": "rollup -c",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "index.js"
      ],
      "extensions": "js,jsx",
      "ignore": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengquan223/dazv.git"
  },
  "keywords": [
    "canvas",
    "visualization",
    "chart"
  ],
  "author": "309581454@qq.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chengquan223/dazv/issues"
  },
  "homepage": "https://github.com/chengquan223/dazv#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.2.0",
    "npm-watch": "^0.1.6",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglify-js": "^2.7.4"
  }
}
